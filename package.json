    "react-query": "^3.39.3",
    "react-typing-effect": "^2.0.5",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.5",
    "start-server-and-test": "^2.0.0",
    "tailwind-merge": "^1.13.1",
    "tailwind-scrollbar-hide": "^1.1.7",
