    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@mui/icons-material": "^5.12.1",
    "@mui/material": "^5.12.1",
    "@mui/styled-engine-sc": "^5.12.0",
    "@next/env": "^13.3.0",
    "@types/js-cookie": "^3.0.3",
    "@types/mocha": "^10.0.1",
    "react-hook-form": "^7.43.1",
