  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.19",
    "postcss-nesting": "^10.2.0",
    "sass": "^1.56.1",
    "tailwindcss": "^3.2.4"
